{
  "name": "pirated-medium.com",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .eslintignore  . --ext .js,.jsx,.ts,.tsx",
